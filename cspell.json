{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "dictionaryDefinitions": [
        {
            "name": "project-words",
            "path": "./project-words.txt",
            "addWords": true
        }
    ],
    "dictionaries": [
        "project-words"
    ],
    "enableFiletypes": [
        "solidity"
    ],
    "ignorePaths": [
        ".husky",
        "artifacts",
        "audits",
        "cache",
        "coverage",
        "docs",
        "node_modules",
        "typechain-types",
        "*.json",
        "project-words",
    ]
}
